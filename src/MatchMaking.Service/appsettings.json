{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Kafka": {
    "Brokers": "localhost:9092",
    "MatchmakingRequestTopic": "matchmaking.requests",
    "MatchmakingCompleteTopic": "matchmaking.complete",
    "MatchmakingCompleteGroup": "matchmaking.complete.services"
  },
  "Cache": {
    "MatchLifetime": 1
  }
}